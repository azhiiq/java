package com.TestngFeatures;

import org.testng.asserts.SoftAssert;

public class Soft_Assert_Concept {
	
	
	
	public void demo_password() {
		String exp_FN = "Mart";
		String exp_LN="Teena";
		
		SoftAssert s = new SoftAssert();
		
		s.assertEquals(exp_FN, exp_LN);
	}
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	

}
